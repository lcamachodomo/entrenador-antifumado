            android:theme="@style/CustomActionBarThemeNoActionBar" >
        </activity>
        <activity
            android:name=".register.RegisterActivityTwo"
            android:label=""
            android:screenOrientation="sensorPortrait"
            android:theme="@style/CustomActionBarThemeNoActionBar" >
